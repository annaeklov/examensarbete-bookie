, []);\n\n  function getUserAxios(loggedInUserId) {\n    axios\n      .get(`http://localhost:3000/user/${loggedInUserId}`)\n      .then((response) => {\n        console.log(response.data);\n        setUserInfo(response.data);\n      })\n      .catch((error) => {\n        console.log(error);\n      });\n  }\n\n  return (\n    <Router>\n      <AppDiv>\n        <Switch />\n        <Route path=\"/login\">\n          <Login />\n        </Route>\n        <Route path=\"/user\">\n          <Profile\n            getUserAxios={getUserAxios}\n            userInfo={userInfo}\n            setUserInfo={setUserInfo}\n          />\n          <TabBar />\n        </Route>\n        <Route path=\"/search\">\n          <Search\n            getUserAxios={getUserAxios}\n            userInfo={userInfo}\n            setUserInfo={setUserInfo}\n          />\n          <TabBar />\n        </Route>\n        <Route exact path=\"/bookclubs\">\n          <Bookclubs\n            getUserAxios={getUserAxios}\n            userInfo={userInfo}\n            setUserInfo={setUserInfo}\n          />\n          <TabBar />\n        </Route>\n        <Route path=\"/bookclubs/:id\">\n          <Bookclub\n            userInfo={userInfo}\n          />\n          <TabBar />\n        </Route>\n        <Switch />\n      </AppDiv>\n    </Router>\n  );\n}\n\nexport default App;\n","/Users/annaeklov/Documents/EC-utbildning/examensarbete/app/client/src/reportWebVitals.js",[],"/Users/annaeklov/Documents/EC-utbildning/examensarbete/app/client/src/components/Login.js",["52","53"],"import React, { useEffect, useState } from \"react\";\nimport axios from \"axios\";\nimport { useHistory, Redirect } from \"react-router-dom\";\n\nfunction Login() {\n  const [password, setPassword] = useState(\"\");\n  const [username, setUsername] = useState(\"\");\n  const [errorMsg, setErrorMsg] = useState(\"\");\n  let history = useHistory();\n\n  useEffect(() => {\n    const loggedInUserId = localStorage.getItem(\"userId\");\n    if (loggedInUserId) {\n      history.push(\"/user\");\n    }\n    return () => {};\n  }, []);\n\n  function postAxios() {\n    const user = { username, password };\n    axios\n      .post(\"http://localhost:3000/login\", user)\n      .then((response) => {\n        localStorage.setItem(\"userId\", response.data.userId);\n        history.push(\"/user\");\n      })\n      .catch((error) => {\n        console.log(error);\n        setErrorMsg(\"Wrong\");\n        setUsername(\"\");\n        setPassword(\"\");\n      });\n  }\n\n  function handleSubmit(e) {\n    e.preventDefault();\n\n    if (username.trim() === \"\" || password.trim() === \"\") {\n      return setErrorMsg(\"Empty\");\n    }\n    postAxios();\n  }\n\n  function onChangeUsername(e) {\n    setUsername(e.target.value);\n  }\n  function onChangePassword(e) {\n    setPassword(e.target.value);\n  }\n\n  return (\n    <div className=\"Login\">\n      <form onSubmit={handleSubmit}>\n        <input\n          type=\"text\"\n          name=\"username\"\n          placeholder=\"Username\"\n          required\n          onChange={onChangeUsername}\n          autoFocus\n          value={username}\n        />\n        <input\n          type=\"password\"\n          required\n          name=\"password\"\n          placeholder=\"Password\"\n          minLength=\"1\"\n          onChange={onChangePassword}\n          value={password}\n        />\n        <input type=\"submit\" value=\"Logga in\" />\n      </form>\n      {errorMsg === \"Empty\" && <p>Input kan inte vara tomt</p>}\n      {errorMsg === \"Wrong\" && <p>Användarnamn eller lösen är fel</p>}\n    </div>\n  );\n}\n\nexport default Login;\n","/Users/annaeklov/Documents/EC-utbildning/examensarbete/app/client/src/components/Profile.js",["54","55","56","57","58"],"import React, { useState, useEffect } from \"react\";\nimport axios from \"axios\";\nimport { useHistory, Redirect, Link } from \"react-router-dom\";\nimport TabBar from \"./TabBar\";\n\nfunction Profile({ getUserAxios